      sp:where (
          [
            rdf:type sp:SubQuery ;
            sp:query [
                rdf:type sp:Select ;
                sp:groupBy (
                    spin:_this
                  ) ;
                sp:resultVariables (
                    spin:_this
                    [
                      sp:expression [
                          rdf:type sp:Count ;
                          sp:expression [
                              sp:varName "a" ;
                            ] ;
                        ] ;
                      sp:varName "nw" ;
                    ]
                  ) ;
                sp:where (
                    [
                      sp:object spin:_this ;
                      sp:predicate sides:is_part_of ;
                      sp:subject [
                          sp:varName "a" ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "p" ;
                        ] ;
                      sp:predicate sides:has_wrongly_ticked ;
                      sp:subject [
                          sp:varName "a" ;
                        ] ;
                    ]
                  ) ;
              ] ;
          ]
        ) ;
    ] ;
